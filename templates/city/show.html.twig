{% extends 'base.html.twig' %}

{% block title %}City{% endblock %}

{% block body %}
    <h1>City</h1>

    <table class="table">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ city.id }}</td>
        </tr>
        <tr>
            <th>CityName</th>
            <td>{{ city.CityName }}</td>
        </tr>
        <tr>
            <th>CountryCode</th>
            <td>{{ city.CountryCode }}</td>
        </tr>
        <tr>
            <th>Latitude</th>
            <td>{{ city.Latitude }}</td>
        </tr>
        <tr>
            <th>Longitude</th>
            <td>{{ city.Longitude }}</td>
        </tr>
        </tbody>
    </table>

    <a href="{{ path('app_city_index') }}" class="btn btn-secondary" style="margin-right: 10px;">Back to list</a>

    <a href="{{ path('app_city_edit', {'id': city.id}) }}" class="btn btn-primary" style="margin-right: 10px;">Edit</a>

    {{ include('city/_delete_form.html.twig') }}
{% endblock %}
